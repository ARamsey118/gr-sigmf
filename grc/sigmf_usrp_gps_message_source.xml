<?xml version="1.0"?>
<block>
  <name>USRP GPS Message Source</name>
  <key>sigmf_usrp_gps_message_source</key>
  <category>[SigMF]</category>
  <import>import sigmf</import>
  <make>sigmf.usrp_gps_message_source($uhd_args, $poll_interval)</make>

  <param>
    <name>UHD Args</name>
    <key>uhd_args</key>
    <type>string</type>
  </param>
  <param>
    <name>Poll Interval (s)</name>
    <key>poll_interval</key>
    <value>1.0</value>
    <type>real</type>
  </param>

  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
